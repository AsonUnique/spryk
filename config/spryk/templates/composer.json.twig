{
  "name": "spryker/{{ module | dasherize }}",
  "description": "{{ module }} module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1"
  },
  "autoload": {
    "psr-4": {
      "{{ organization }}\\": "src/{{ organization }}/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
